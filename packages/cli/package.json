{
    "name": "mcpget",
    "version": "0.1.0",
    "description": "MCP configuration installer for AI coding tools",
    "type": "module",
    "bin": {
        "mcpget": "./dist/cli.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsx src/cli.ts",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "mcp-installer",
        "mcp-config",
        "cli",
        "ai-tools",
        "ai-agents",
        "claude-code",
        "cursor",
        "windsurf",
        "antigravity",
        "gemini-cli",
        "vscode",
        "zed",
        "cline",
        "roo-code",
        "continue",
        "codex",
        "goose",
        "opencode",
        "github-copilot"
    ],
    "license": "MIT",
    "dependencies": {
        "@clack/prompts": "^0.11.0",
        "gray-matter": "^4.0.3",
        "js-yaml": "^4.1.0",
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.0",
        "tsup": "^8.3.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=18"
    }
}